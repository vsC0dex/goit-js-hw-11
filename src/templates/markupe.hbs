{{#each this}}
<div class="photo-card">
  <a href="{{largeImageURL}}"><img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" /></a>
  <div class="info">
    <p class="info-item">
      <b>likes:</b>{{likes}}
    </p>
    <p class="info-item">
      <b>views:</b>{{views}}
    </p>
    <p class="info-item">
      <b>comments:</b>{{comments}}
    </p>
    <p class="info-item">
      <b>downloads:</b>{{downloads}}
    </p>
  </div>
</div>
{{/each}}